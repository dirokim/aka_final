<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.aka.app.payment.PaymentDAO">


	<insert id="createPayment" parameterType="PaymentVO">
	INSERT INTO PAYMENT (PRODUCT_NUM,MEMBER_ID,ORDER_ID,AMOUNT,PAYMENT_KEY,ORDER_NAME,REQUESTED_AT,APPROVED_AT,COUNTRY,CURRENCY,METHOD,VAT) 
	VALUES (#{product_num},#{member_id},#{order_id},#{amount},#{payment_key},#{order_name},#{requested_at},#{approved_at},#{country},#{currency},#{method},#{vat})
	</insert>



	
	
	

</mapper>